    "@react-navigation/native-stack": "^6.7.0",
    "@tanstack/react-query": "^4.1.3",
    "@types/react": "~18.0.0",
    "@types/react-native": "~0.69.1",
    "expo": "~46.0.7",
    "expo-status-bar": "~1.4.0",
    "expo-svg-uri": "^1.3.1",
    "expo-updates": "~0.14.4",
